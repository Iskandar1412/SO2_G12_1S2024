#! /usr/bin/env stap



probe syscall.mmap2 {
    ts = gettimeofday_s();
    length = length; 
    //pid, nombre, proceso, fecha, length
    printf("%d,%s,%s,%s,%d\n", pid(), execname(), "mmap", ctime(ts), length)
}

